	<insert id="setItemBase" parameterType="egovframework.aviation.metadata.vo.param.MetaDataParamVO">
			insert into nam_item_base (
				<include refid="metadata_columns1"><property name="alias" value=""/></include>
			)
			values (
				<include refid="metadata_columns2"><property name="alias" value=""/></include>
			);
			<selectKey keyProperty="item_idx" order="AFTER" resultType="int"> 
				 SELECT max(item_idx) from nam_item_base 
			</selectKey>
	</insert>
	
	<insert id="setTaxonomy" parameterType="egovframework.aviation.metadata.vo.param.MetaDataParamVO">
			insert into nam_taxonomy (
				item_idx, class1_code_idx, class2_code_idx, class3_code_idx, reg_user
			) 
			values (
				#{item_idx}, #{class1_code_idx}, #{class2_code_idx}, #{class3_code_idx}, #{reg_user}
			)
	</insert>
	
	<insert id="setCountry" parameterType="egovframework.aviation.metadata.vo.param.MetaDataParamVO">
			insert into nam_material (
				item_idx, material1_code_idx, material2_code_idx, material_detail, reg_user
			) 
			values (
				#{item_idx}, #{material1_code_idx}, #{material2_code_idx}, #{material_detail}, #{reg_user}
			)
	</insert>
	
	<insert id="setMaterial" parameterType="egovframework.aviation.metadata.vo.param.MetaDataParamVO">
			insert into nam_country_era (
